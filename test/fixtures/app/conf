
app.middleware.available = {

  public = {

    module = express#static('${__dirname}/public', {maxAge=0})

  }

  json = {

    module = body-parser|json) 

  }

  urlencoded = {

    module = $(body-parser|urlencoded) 

  }

}

app.modules = {

  r = ./modules/r

}

app.middleware.enabled = [ 'public', 'json', 'urlencoded']
